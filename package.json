{
  "name": "dollar1-unistroke-recognizer",
  "description": "A wrapper of $1 Unistroke Recognizer.",
  "version": "0.1.0",
  "repository": "2players/dollar1-unistroke-recognizer",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "test": "ava",
    "code-quality": "npm run lint && npm run test",
    "cleanup": "rm -rf dist",
    "transpile": "babel src -d dist",
    "copy-files": "cp -r package.json README.md dist",
    "build": "npm run cleanup && npm run transpile && npm run copy-files",
    "release": "npm run code-quality && npm run build && cd dist && npm publish"
  },
  "files": [
    "dist/"
  ],
  "main": "index.js",
  "keywords": [
    ""
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "1.0.0-rc.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-ava": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "esm": "^3.0.84",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.11.1"
  },
  "ava": {
    "require": [
      "./test/_register.js",
      "esm"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{json,md}": [
        "prettier --write",
        "git add"
      ],
      "**/*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "license": "MIT"
}
